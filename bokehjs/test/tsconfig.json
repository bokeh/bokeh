{
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "noEmitOnError": false,
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES5",
    "lib": ["es5", "dom", "es2015.collection"],
    "baseUrl": ".",
    "paths": {
      "*": ["../build/js/tree/*", "../build/js/types/*", "../src/lib/external/*"]
    },
    "outDir": "../build/test"
  },
  "include": ["./**/*.ts"]
}

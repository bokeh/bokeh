{
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "noEmitOnError": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES5",
    "lib": ["es5", "dom", "es2015.collection"],
    "declaration": true,
    "importHelpers": true,
    "jsx": "react",
    "reactNamespace": "DOM",
    "experimentalDecorators": true,
    "types": [],
    "baseUrl": "./build/js/tree_ts/"
  }
}

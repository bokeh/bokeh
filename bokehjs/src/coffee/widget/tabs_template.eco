<ul class="bk-bs-nav bk-bs-nav-tabs">
  <% for tab, i in @tabs: %>
    <li class="<%= @active(i) %>">
      <a data-index="<%= i %>" href="#tab-<%= tab.get('id') %>"><%= tab.get('title') %></a>
    </li>
  <% end %>
</ul>
<div class="bk-bs-tab-content">
  <% for tab, i in @tabs: %>
    <div class="bk-bs-tab-pane <%= @active(i) %>" id="tab-<%= tab.get('id') %>"></div>
  <% end %>
</div>

{
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "strictNullChecks": false,
    "alwaysStrict": true,
    "noEmitOnError": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES5",
    "lib": ["es5", "dom", "es2015.collection"],
    "declaration": false,
    "importHelpers": false,
    "experimentalDecorators": true,
    "types": []
  },
  "compileOnSave": false
}
